{
  "name": "aqayc",
  "private": true,
  "scripts": {
    "clean": "yarn run delete-eslint-cache && yarn run delete-node-modules && yarn run delete-build && yarn",
    "delete-eslint-cache": "find . -name '*.eslintcache' -type f -delete ",
    "delete-node-modules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "delete-build": "lerna run delete-build"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "lerna": "^4.0.0",
    "yarn": "^1.22.10"
  },
  "workspaces": [
    "packages/*"
  ],
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/amine-abdelli/aqac.git",
  "author": "Amine Abdelli <amine.abdelli@outlook.fr>",
  "license": "MIT"
}