{
  "name": "aqayc",
  "private": true,
  "scripts": {
    "start": "./node_modules/.bin/turbo run start",
    "start:prod": "./node_modules/.bin/turbo run start:prod",
    "start:docker": "docker-compose --file docker-compose.dev.yml up --build",
    "start:docker-prod": "docker-compose --file docker-compose.prod.yml up --build",
    "test": "./node_modules/.bin/turbo run test",
    "lint": "./node_modules/.bin/turbo run lint",
    "build": "./node_modules/.bin/turbo run build",
    "clean": "yarn run delete-eslint-cache && yarn run delete-node-modules && yarn",
    "delete-eslint-cache": "find . -name '*.eslintcache' -type f -delete ",
    "delete-node-modules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "yarn": "^1.22.10"
  },
  "workspaces": [
    "packages/*"
  ],
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/amine-abdelli/aqac.git",
  "author": "Amine Abdelli <amine.abdelli@outlook.fr>",
  "license": "MIT",
  "devDependencies": {
    "turbo": "^1.2.11"
  }
}
